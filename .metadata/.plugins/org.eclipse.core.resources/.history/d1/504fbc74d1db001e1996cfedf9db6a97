@namespace( uri="http://abstractmodel", prefix="p")
package abstractmodel;

class ModelFactoryAbstract {
    val ProjectAdj [*] listProjects;
}

class ProjectAdj{
    attr String name;
    attr String path;
    val ClassTypeFactoryAdj classTypeFactoryAdj;
    val AttributeTypeFactoryAdj attributeTypeFactoryAdj;
    val PackageAdj [*] lstPackageAdj;       
}

class ClassTypeFactoryAdj{   
    val   ClassTypeAdj [*] lstClassTypeAdj;   
}

class ClassTypeAdj{
    attr String name;   
    val ClassAdj [*] lstClassAdj;
       
}

class AttributeTypeFactoryAdj{   
    val   AttributeTypeAdj [*] lstAttributeTypeAdj;   
}

class AttributeTypeAdj {   
    attr String name;   
    val ParameterAdj [*] lstParameterAdj;
    val AttributeAdj [*] lstAttributeAdj;
}

class PackageAdj  {
    attr String name;
    attr String path;
    val PackageAdj [*] lstChildPackageAdj;   
    val ClassAdj [*] lstClassAdj;
}

class ClassAdj {
    attr String name;
    ref  ClassTypeAdj classTypeAdj;
    val AttributeAdj [*] lstAttributeAdj;
    val MethodAdj [*] lstMethodAdj;
    val RelationshipAdj[*] lstRelationShipInAdj;//entran
    val RelationshipAdj[*] lstRelationShipExitAdj;//salen
}


class RelationshipAdj{
    attr String roleTarget;
    attr String roleSource;
    attr String multiplicityTargetClass;
    attr String multiplicitySourceClass;
    ref ClassAdj targetClass;
    
}

class AttributeAdj {
    attr String name;
    attr String valor;
    ref AttributeTypeAdj attributeTypeAdj;
    
}

class MethodAdj{
    attr String methodName;
    attr AccessModifier accessModify;
    attr String returnType;
    attr String body;
    val ParameterAdj [*] lstParameterAdj;

}

class ParameterAdj {
    attr String name;
    ref AttributeTypeAdj attributeTypeAdj;
}


enum AccessModifier {   
    PUBLIC=1;
    PRIVATE=2;
    PROTECTED=3;
}


