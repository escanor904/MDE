public static int getLinkWithClassVisualID(org.eclipse.emf.ecore.EObject domainElement) {
	if (domainElement == null) {
		return -1;
	}
	if (concretemodel.ConcretemodelPackage.eINSTANCE.getContainmentSdj().isSuperTypeOf(domainElement.eClass())) {
	return concretemodel.diagram.edit.parts.ContainmentSdjEditPart.VISUAL_ID;
}
if (concretemodel.ConcretemodelPackage.eINSTANCE.getContainmentSdj().isSuperTypeOf(domainElement.eClass())) {
	return concretemodel.diagram.edit.parts.ContainmentSdj2EditPart.VISUAL_ID;
}
if (concretemodel.ConcretemodelPackage.eINSTANCE.getSharingAdj().isSuperTypeOf(domainElement.eClass())) {
	return concretemodel.diagram.edit.parts.SharingAdjEditPart.VISUAL_ID;
}
if (concretemodel.ConcretemodelPackage.eINSTANCE.getSharingAdj().isSuperTypeOf(domainElement.eClass())) {
	return concretemodel.diagram.edit.parts.SharingAdj2EditPart.VISUAL_ID;
}
if (concretemodel.ConcretemodelPackage.eINSTANCE.getAssociationAdj().isSuperTypeOf(domainElement.eClass())) {
	return concretemodel.diagram.edit.parts.AssociationAdjEditPart.VISUAL_ID;
}
if (concretemodel.ConcretemodelPackage.eINSTANCE.getAssociationAdj().isSuperTypeOf(domainElement.eClass())) {
	return concretemodel.diagram.edit.parts.AssociationAdj2EditPart.VISUAL_ID;
}
if (concretemodel.ConcretemodelPackage.eINSTANCE.getGeneralizationAdj().isSuperTypeOf(domainElement.eClass())) {
	return concretemodel.diagram.edit.parts.GeneralizationAdjEditPart.VISUAL_ID;
}
if (concretemodel.ConcretemodelPackage.eINSTANCE.getGeneralizationAdj().isSuperTypeOf(domainElement.eClass())) {
	return concretemodel.diagram.edit.parts.GeneralizationAdj2EditPart.VISUAL_ID;
}
	return -1;
}
	
